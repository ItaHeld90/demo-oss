  "dependencies": {
    "unique-random-array": "1.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "ghooks": "2.0.4",
    "mocha": "5.2.0",
